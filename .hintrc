{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[playsinline]",
          "img[fetchpriority]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: none",
          "user-select"
        ]
      }
    ],
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}